<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0">
	<persistence-unit name="cetechUnit">
	
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>jdbc/cetechds</jta-data-source>
		
		<class>mc.com.Cetech.system.model.CatOcupation</class>
		
		<properties>
			<property name="eclipselink.loggin.logger" value="ServerLogger"/>
			<property name="eclipselink.loggin.level" value="FINE"/>
			<property name="eclipselink.loggin.leve.sql" value="FINE"/>
		</properties>
	
	</persistence-unit>


</persistence>